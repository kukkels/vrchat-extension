{
	"manifest_version": 2,
	"name": "Vrchat Website Extension",
	"description": "An extension to extend/improve the vrchat website functionality",
	"version": "1.0",
	"permissions": [
		"https://*.vrchat.net/*"
	],
	"content_scripts": [
		{
			"matches": [
				"https://*.vrchat.net/*"
			],
			"js": [
				"build/app.js"
			],
			"css": [
				"build/main.css"
			],
			"run_at": "document_start"
		}
	]
}
